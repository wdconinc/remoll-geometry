<?xml version="1.0" encoding="UTF-8"?>
<gdml xmlns:gdml="http://cern.ch/2001/Schemas/GDML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/gdml.xsd">

  <materials>
    <material Z="1" name="Vacuum" state="gas">
      <T unit="K" value="2.73"/>
      <P unit="pascal" value="3e-18"/>
      <D unit="g/cm3" value="1e-25"/>
      <atom unit="g/mole" value="1.01"/>
    </material>
    <material Z="1" name="PlaneVacuum" state="gas">
      <T unit="K" value="2.73"/>
      <P unit="pascal" value="3e-18"/>
      <D unit="g/cm3" value="1e-25"/>
      <atom unit="g/mole" value="1.01"/>
    </material>
  </materials>
  
  <solids>
    <box lunit="mm" name="GEMDetBox" x="3000" y="3000" z="500"/>
    <cone aunit="deg" deltaphi="360" lunit="mm" name="GEMPlaneCone" rmax1="1000" rmax2="1000" rmin1="0" rmin2="0" startphi="0" z="50"/>
  </solids>
  
  <structure>
    
    <volume name="logicalVol_GEMp1b_1">
      <materialref ref="PlaneVacuum"/>
      <solidref ref="GEMPlaneCone"/>
      <auxiliary auxtype="SensDet" auxvalue="GEMPlane"/>
      <auxiliary auxtype="DetNo" auxvalue="506"/>
    </volume>
    
    <volume name="logicalVol_GEMp1b_2">
      <materialref ref="PlaneVacuum"/>
      <solidref ref="GEMPlaneCone"/>
      <auxiliary auxtype="SensDet" auxvalue="GEMPlane"/>
      <auxiliary auxtype="DetNo" auxvalue="507"/>
    </volume>

    <volume name="logicalVol_GEMp1b_3">
      <materialref ref="PlaneVacuum"/>
      <solidref ref="GEMPlaneCone"/>
      <auxiliary auxtype="SensDet" auxvalue="GEMPlane"/>
      <auxiliary auxtype="DetNo" auxvalue="508"/>
    </volume>

    <volume name="logicalVol_GEMp1b_4">
      <materialref ref="PlaneVacuum"/>
      <solidref ref="GEMPlaneCone"/>
      <auxiliary auxtype="SensDet" auxvalue="GEMPlane"/>
      <auxiliary auxtype="DetNo" auxvalue="509"/>
    </volume>

    <volume name="logicalVol_GEMp1b_5">
      <materialref ref="PlaneVacuum"/>
      <solidref ref="GEMPlaneCone"/>
      <auxiliary auxtype="SensDet" auxvalue="GEMPlane"/>
      <auxiliary auxtype="DetNo" auxvalue="510"/>
    </volume>
    
    <volume name="logicalVol_GEMDet1b">
      <materialref ref="Vacuum"/>
      <solidref ref="GEMDetBox"/>

      <physvol name="physVol_GEMp1b_1">
        <volumeref ref="logicalVol_GEMp1b_1"/>
        <position name="logicalVol_GEMp1_pos" unit="mm" x="0" y="0" z="-200"/>
      </physvol>	

      <physvol name="physVol_GEMp1b_2">
        <volumeref ref="logicalVol_GEMp1b_2"/>
        <position name="logicalVol_GEMp2_pos" unit="mm" x="0" y="0" z="-100"/>
      </physvol>	

      <physvol name="physVol_GEMp1b_3">
        <volumeref ref="logicalVol_GEMp1b_3"/>
        <position name="logicalVol_GEMp3_pos" unit="mm" x="0" y="0" z="0"/>
      </physvol>	

      <physvol name="physVol_GEMp1b_4">
        <volumeref ref="logicalVol_GEMp1b_4"/>
        <position name="logicalVol_GEMp4_pos" unit="mm" x="0" y="0" z="100"/>
      </physvol>	

      <physvol name="physVol_GEMp1b_5">
        <volumeref ref="logicalVol_GEMp1b_5"/>
        <position name="logicalVol_GEMp5_pos" unit="mm" x="0" y="0" z="200"/>
      </physvol>	
      
    </volume>
  </structure>
  
  <setup name="Default" version="1.0">
    <world ref="logicalVol_GEMDet1b"/>
  </setup>
  
</gdml>

